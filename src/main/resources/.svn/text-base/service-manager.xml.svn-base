<service-mappings>

    <service-mapping interface="com.yahoo.satg.bp.exch.pod.NetworkQualityService" default="service">
        <service implementation="com.yahoo.satg.bp.exch.networkquality.NetworkQualityServiceImpl" n-instances="1" />
    </service-mapping>
    
    <service-mapping interface="com.yahoo.satg.bp.cust.pod.AccountsPodService" default="service">
        <service implementation="com.yahoo.satg.bp.cust.AccountsPodServiceImpl" n-instances="1" />
    </service-mapping>

	<!-- Added service mappings for real user context in tests -->
    <service-mapping interface="com.yahoo.idm.IDMPodService" default="service">        
    	<service implementation="com.yahoo.idm.IDMPodBeanEJB" n-instances="1" />        
    	<ejb-local jndi-name="com.yahoo.idm.IDMPodBeanLocal" />
    </service-mapping>
    
    <service-mapping interface="com.yahoo.sm.ads.common.app.PodService">        
    	<service implementation="com.yahoo.sm.ads.common.app.PodServiceImpl" n-instances="1" />
    </service-mapping>
    
    <service-mapping interface="com.yahoo.sm.ads.idm.LoginService">
        <service implementation="com.yahoo.sm.ads.idm.LoginServiceImpl" n-instances="1" />
    </service-mapping>

</service-mappings>
